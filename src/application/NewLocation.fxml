<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NewSearchController">
   <children>
      <Label alignment="CENTER" layoutX="50.0" layoutY="29.0" prefHeight="66.0" prefWidth="405.0" text="New Location File Setup" textAlignment="CENTER" textFill="#3911d6" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="31.0" />
         </font>
      </Label>
      <TextField fx:id="searchTxtBox" layoutX="26.0" layoutY="165.0" onAction="#searchTextBoxAction" prefHeight="25.0" prefWidth="196.0" promptText="Type Location Name or Address" />
      <Label layoutX="26.0" layoutY="127.0" prefHeight="38.0" prefWidth="154.0" text="Enter a Location Name or Address:" wrapText="true">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="95.0" prefHeight="38.0" prefWidth="167.0" text="Location Search" textAlignment="CENTER" textFill="#3911d6" underline="true">
         <font>
            <Font name="Bookman Old Style Bold" size="18.0" />
         </font>
      </Label>
      <ListView fx:id="searchResults" layoutX="24.0" layoutY="201.0" prefHeight="200.0" prefWidth="314.0" />
      <Button fx:id="addToFile" layoutX="24.0" layoutY="415.0" mnemonicParsing="false" text="Add to File" />
   </children>
</AnchorPane>
